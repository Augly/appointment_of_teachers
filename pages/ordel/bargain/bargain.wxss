.head {
  width: 100%;
  height: 360rpx;
  background: linear-gradient(91deg, rgba(104, 227, 245, 1) 0%, rgba(123, 235, 221, 1) 100%);
  box-sizing: border-box;
  padding: 0rpx 24rpx;
  overflow: hidden;
}

.head_wrap {
  width: 100%;
  height: 316rpx;
  background-color: rgba(255, 255, 255, 1);
  margin-top: 44rpx;
  border-radius: 8rpx 8rpx 0rpx 0rpx;
  box-sizing: border-box;
  padding: 48rpx 26rpx;
}

.head_top {
  width: 100%;
  height: 96rpx;
  display: flex;
  align-items: center;
}

.top_val {
  width: 96rpx;
  height: 96rpx;
}

.top_res {
  /* width: 450rpx; */
  height: auto;
  display: flex;
  flex-direction: column;
  margin-left: 16rpx;
}

.top_res_title {
  display: flex;
  align-items: center;
}

.top_res_name {
  font-size: 34rpx;
  color: rgba(44, 44, 44, 1);
}

.top_res_label {
  height: 40rpx;
  line-height: 40rpx;
  padding: 0rpx 16rpx;
  background-color: rgba(96, 234, 243, 1);
  font-size: 26rpx;
  margin-left: 8rpx;
  color: rgba(255, 255, 255, 1);
  border-radius: 4rpx;
}

.top_res_detail {
  font-size: 22rpx;
  color: rgba(153, 153, 153, 1);
  margin-top: 2rpx;
}

.icon-arr-right-red {
  font-size: 26rpx;
  color: rgba(96, 234, 243, 1);
  margin-left: auto;
}

.head_bottom {
  width: 100%;
  height: auto;
  display: flex;
  align-items: baseline;
  justify-content: center;
  margin-top: 48rpx;
}

.bottom_title {
  font-size: 34rpx;
  color: rgba(255, 153, 73, 1);
}

.bottom_price {
  font-size: 48rpx;
  color: rgba(255, 153, 73, 1);
  margin-left: 30rpx;
}

.bottom_price::before {
  content: '￥';
  font-size: 24rpx;
  color: rgba(255, 153, 73, 1);
}

.content {
  width: 100%;
  height: auto;
  background-color: RGBA(223, 243, 250, 1);
  box-sizing: border-box;
  padding: 40rpx;
}

.button_group {
  width: 528rpx;
  height: 88rpx;
  border-radius: 8rpx;
  background: linear-gradient(90deg, rgba(104, 227, 245, 1) 0%, rgba(123, 235, 221, 1) 100%);
  margin: 0 auto;
  margin-top: 40rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 88rpx;
  color: rgba(255, 255, 255, 1);
}

.button_return {
  width: 528rpx;
  height: 88rpx;
  background: rgba(255, 255, 255, 1);
  border: 2rpx solid rgba(96, 234, 243, 1);
  opacity: 1;
  border-radius: 8rpx;
  margin: 0 auto;
  margin-top: 40rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 88rpx;
  color: rgba(96, 234, 243, 1);
}

.content_wrap {
  width: 100%;
  min-height: 694rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
  margin-top: 40rpx;
  box-sizing: border-box;
  padding: 50rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow: hidden;
  margin-bottom: 168rpx;
}

.title {
  font-size: 32rpx;
  color: rgba(255, 153, 73, 1);
}

.margin0 {
  margin: 0 auto;
}

.bottom {
  width: 100%;
  height: 168rpx;
  background-color: rgba(223, 243, 250, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  bottom: 0;
}

.wrap_title {
  font-size: 32rpx;
  color: rgba(255, 153, 73, 1);
}

.wrap_title::before {
  content: '——— ';
  color: rgba(255, 153, 73, 1);
}

.wrap_title::after {
  content: ' ———';
  color: rgba(255, 153, 73, 1);
}

.wrap_list {
  width: 100%;
  height: 58rpx;
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}

.list_val {
  width: 58rpx;
  height: 58rpx;
}

.list_name {
  color: rgba(44, 44, 44, 1);
  font-size: 24rpx;
  margin-left: 10rpx;
}

.list_price {
  color: rgba(255, 153, 73, 1);
  font-size: 30rpx;
  margin-left: auto;
}

.list_price::before {
  content: '￥';
  font-size: 16rpx;
  color: rgba(255, 153, 73, 1);
}

.mask {
  top: 0;
  width: 100%;
  height: 100%;
  position: fixed;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  background-color: rgba(0, 0, 0, 0.6);
}

.mask_content {
  width: 528rpx;
  height: 684rpx;
  background-color: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
  overflow: hidden;
}

.share_group {
  width: 100%;
  height: 480rpx;
  background-color: red;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.share_code {
  width: 272rpx;
  height: 272rpx;
  background: rgba(0, 0, 0, 1);
  border-radius: 50%;
}
.share_title{
  font-size:34rpx;
  color: rgba(255, 255, 255, 1); 
}
.share_title{
  font-size:34rpx;
  color: rgba(255, 255, 255, 1); 
  margin-top: 26rpx;
}
.share_mintitle{
  font-size:70rpx;
  color: rgba(255, 255, 255, 1); 
}
.share_res{
  width: 100%;
  height: 204rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 0rpx 0rpx 0rpx 44rpx;
}