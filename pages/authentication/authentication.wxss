page {
  box-sizing: border-box;
  padding: 0rpx 32rpx;
}

.step_group {
  /* width: 500rpx; */
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  margin-top: 48rpx;
}

.step {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  border: 2rpx solid rgba(153, 153, 153, 1);
  font-size: 32rpx;
  color: rgba(153, 153, 153, 1);
  display: flex;
  align-items: center;
  justify-content: center;
}

.step_active {
  color: rgba(255, 255, 255, 1);
  background: rgba(96, 234, 243, 1);
  border: 2rpx solid rgba(96, 234, 243, 1);
}

.line {
  width: 152rpx;
  height: 0rpx;
  border: 2rpx dashed rgba(153, 153, 153, 1);
}

.tip {
  color: rgba(51, 51, 51, 1);
  font-size: 26rpx;
  display: flex;
  align-items: center;
  margin-top: 66rpx;
}

.tip_click {
  color: rgba(96, 234, 243, 1);
  font-size: 26rpx;
  text-decoration: underline;
}

.button {
  width: 688rpx;
  height: 88rpx;
  background: rgba(96, 234, 243, 1);
  border-radius: 8rpx;
  text-align: center;
  line-height: 88rpx;
  color: rgba(255, 255, 255, 1);
  font-size: 34rpx;
  margin-top: 112rpx;
}

.up_wrap {
  width: 296rpx;
  height: 296rpx;
  border: 2rpx solid rgba(204, 204, 204, 1);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  margin-top: 65rpx;
}
.icon-hao{
  font-size: 70rpx;
  color: rgba(183, 183, 183, 1);
}
.title{
  font-size: 36rpx;
  color: rgba(183, 183, 183, 1);
  margin-top: 30rpx;
}
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 5;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.mask_tip{
  width: 520rpx;
  height: 360rpx;
  background-color: rgba(255, 255, 255, 1);
  margin-top: 26rpx;
}
.mask_title{
  font-size: 32rpx;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  margin-top: 48rpx;
  margin-bottom: 12rpx;
}
.mask_mintitle{
   font-size: 26rpx;
  color: rgba(255, 255, 255, 1);
  text-align: center;
}