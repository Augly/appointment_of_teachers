.list_wrap {
  width: 100%;
  height: auto;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 0rpx 32rpx;
  background-color: rgba(255, 255, 255, 1);
}

.list {
  width: 100%;
  height: 96rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: end;
  -ms-flex-pack: end;
  justify-content: flex-end;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;

  border-bottom: 2rpx solid rgba(245, 245, 245, 1);
}

