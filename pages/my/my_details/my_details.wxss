page {
  background-color: rgba(223, 243, 250, 1);
}

.list_wrap {
  width: 100%;
  height: auto;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 0rpx 32rpx;
  background-color: rgba(255, 255, 255, 1);
}

.list {
  width: 100%;
  height: 96rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: end;
  -ms-flex-pack: end;
  justify-content: flex-end;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;

  border-bottom: 2rpx solid rgba(245, 245, 245, 1);
}

.icon-arr-right-red {
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}

.list_val {
  height: 156rpx;
}

.val_logo {
  width: 122rpx;
  height: 122rpx;
  border-radius: 50%;
}

.white {
  background-color: rgba(255, 255, 255, 1);
}

.title {
  font-size: 32rpx;
  color: rgba(44, 44, 44, 1);
  margin-right: auto;
}

.mintitle {
  font-size: 28rpx;
  color: rgba(44, 44, 44, 0.5);
  text-align: right;
  margin-right: 12rpx;
}

.out_group {
  width: 750rpx;
  height: 96rpx;
  background: rgba(231, 73, 122, 1);
  font-size: 30rpx;
  color: rgba(255, 255, 255, 1);
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  margin-top: 96rpx;
}