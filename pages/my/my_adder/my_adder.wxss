page{
  background-color: #F2F2F2;
}
.list{
  width: 100%;
  height: auto;
  background-color: white;
  margin-top: 20rpx;
}
.list_head{
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 28rpx 30rpx;
  border-bottom:2rpx solid #F2F2F2; 

}
.list_title{
  font-size: 28rpx;
  color: #000000;

}

.list_res{
  font-size: 28rpx;
  color: rgba(153, 153, 153, 1);
  margin-top: 24rpx;
}
.list_scope{
  width: 100%;
  height: 88rpx;
  box-sizing: border-box;
  padding: 0rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.scope_left{
  width: auto;
  font-size: 28rpx;
  margin-right: auto;
}
.scope_view{
   width: auto;
  font-size: 28rpx;
  margin-left: 50rpx;
  color: rgba(red, green, blue, alpha)
}
.icon-unxuanze{
  font-size: 28rpx;
  margin-right: 15rpx;
}
.icon-xuanze{
  font-size: 28rpx;
  margin-right: 15rpx;
  color: rgba(96, 234, 243, 1);
}
.icon-bianji,.icon-shanchu{
   font-size: 28rpx;
  margin-right: 15rpx;
}
