/* pages/my/about_us/about_us.wxss */
page{
  background-color: rgba(223, 243, 250, 1);
}
.content{
  width: 100%;
  height: 100%;
}
.wrap{
  width: 100%;
  height: 544rpx;
  background-color: rgba(255, 255, 255, 1);
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}
.wrap_logo{
  width: 260rpx;
  height: 260rpx;
  border-radius:50%;
  background-color: RGBA(96, 234, 243, 1);
  box-shadow: 12rpx  12rpx 24rpx  rgba(180, 232, 235, 1)
}
.title{
  color: rgba(44, 44, 44, 1);
  font-size: 44rpx;
  margin-top: 50rpx;
}
.min_title{
  color: rgba(118, 124, 143, 1);
  font-size: 22rpx;
  margin-top: 18rpx;
}
.list{
  width: 100%;
  height: 96rpx;
  background-color: rgba(255, 255, 255, 1);
  box-sizing: border-box;
  padding: 0rpx 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 16rpx;
}
.list_title{
  font-size: 28rpx;
  color: rgba(44, 44, 44, 1);
}
.icon-arr-right-red{
  font-size: 24rpx;
  color: rgba(96, 234, 243, 1);
}