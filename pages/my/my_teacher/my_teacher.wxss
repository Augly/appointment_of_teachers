.head{
  width: 100%;
  height: 80rpx;
  background-color: rgba(223, 243, 250, 1);
  box-sizing: border-box;
  padding: 0rpx 32rpx;
  font-size: 26rpx;
  color: rgba(153, 153, 153, 1);
  display: flex;
  align-items: center;
}
.list_wrap{
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 0rpx 32rpx;
}
.list{
  height: 160rpx;
  width: 100%;
  display: flex;
  align-items: center;
}
.top_val {
  width: 96rpx;
  height: 96rpx;
}

.top_res {
  /* width: 450rpx; */
  display: flex;
  flex-direction: column;
  margin-left: 16rpx;
}

.top_res_title {
  width: 568rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  margin-top: 5rpx;
}

.top_res_name {
  font-size: 34rpx;
  color: rgba(44, 44, 44, 1);
  margin-right: auto;
}

.top_res_label {
  height: 40rpx;
  line-height: 40rpx;
  padding: 0rpx 16rpx;
  background-color: rgba(96, 234, 243, 1);
  font-size: 26rpx;
  margin-left: 8rpx;
  color: rgba(255, 255, 255, 1);
  border-radius: 4rpx;
}

.top_res_detail {
  font-size: 22rpx;
  color: rgba(153, 153, 153, 1);
  margin-top: 2rpx;
}