page {
  position: relative;
  width: 100%;
  height: 100%;
}

.content {
  width: 100%;
  height: 100%;
  overflow: hidden;
  background-color: RGBA(217, 243, 248, 1);
}

.list {
  width: 610rpx;
  height: 80rpx;
  border-bottom: 2rpx solid rgba(96, 234, 243, 1);
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  margin: 0 auto;
  margin-top: 10rpx;
}

.marT170 {
  margin-top: 170rpx;
}

.ipt_group {
  width: 300rpx;
  height: 80rpx;
  font-size: 32rpx;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 0rpx 10rpx;
}

.title {
  font-size: 32rpx;
  color: rgba(96, 234, 243, 1);
}

.code_wrap {
  width: 160rpx;
  height: 56rpx;
  background-color: rgba(96, 234, 243, 1);
  font-size: 24rpx;
  text-align: center;
  line-height: 56rpx;
  color: white;
  margin-left: auto;
}

.colorBlue {
  color: rgba(96, 234, 243, 1);
}

.button {
  width: 417rpx;
  height: 75rpx;
  background: -webkit-gradient(linear, left top, right top, from(rgba(104, 227, 245, 1)), to(rgba(123, 235, 221, 1)));
  background: linear-gradient(90deg, rgba(104, 227, 245, 1) 0%, rgba(123, 235, 221, 1) 100%);
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  font-size: 32rpx;
  color: rgba(255, 255, 255, 1);
  margin-top: 152rpx;
}

.buttom {
  position: fixed;
  bottom: 0;
  width: 750rpx;
  height: 96rpx;
  text-align: center;
  line-height: 96rpx;
  font-size: 32rpx;
  color: rgba(255, 255, 255, 1);
  background: rgba(96, 234, 243, 1);
}