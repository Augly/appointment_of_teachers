page{
  background-color: rgba(223, 243, 250, 1);
  box-sizing: border-box;
  padding: 25rpx;
}
.list{
  width: 100%;
  height:223rpx;
background:rgba(255,255,255,1);
box-shadow:0rpx 0rpx 14rpx 1rpx rgba(191,191,191,0.15);
border-radius:10rpx;
margin-bottom: 25rpx;
display: flex;
justify-content: space-between;
align-items: center;
box-sizing: border-box;
padding: 40rpx 30rpx;
}
.title{
  font-size:28rpx;
font-family:PingFang-SC-Bold;
font-weight:bold;
color:rgba(96,234,243,1);
}
.date{
  font-size:20rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(96,234,243,1);
}
.content{
  font-size:30rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(44,44,44,1);
margin-top: 30rpx;
}