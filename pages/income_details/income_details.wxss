page {
  box-sizing: border-box;
  padding: 32rpx 24rpx;
  background-color: rgba(223, 243, 250, 1);
}

.content{
  width: 100%;
  height: 784rpx;
  background-color: rgba(255, 255, 255, 1);
  border-radius:8rpx;
  box-sizing: border-box;
  padding: 24rpx;
}
.content_title{
  font-size: 64rpx;
  color: rgba(44, 44, 44, 1);
  font-weight: bold;
  text-align: center;
  margin: 0 auto;
  margin-top: 80rpx;
}
.content_mintitle{
  font-size: 24rpx;
  color: rgba(44, 44, 44, 0.8);
  text-align: center;
  margin: 0 auto;
  margin-bottom: 50rpx;
}
.list{
  width: 100%;
  height: 94rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.list_title{
  font-size: 28rpx;
  color: rgba(44, 44, 44, 1);
}
.list_mintitle{
  font-size: 28rpx;
  color: rgba(153, 153, 153, 1);
}
.marT50{
  margin-top: 50rpx;
}
.tip{
  font-size: 24rpx;
  color: rgba(51, 51, 51, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 60rpx;
}
.call_me{
  font-size: 24rpx;
  color: rgba(96, 234, 243, 1);
}