page {
  position: relative;
  height: 100%;
  width: 100%;
}

.content {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 0rpx 32rpx;
}

.title {
  font-size: 26rpx;
  color: rgba(44, 44, 44, 1);
  line-height: 100rpx;
}

.textWrap {
  width: 688rpx;
  height: 336rpx;
  background: rgba(234, 234, 234, 1);
  border: 2rpx solid rgba(230, 230, 230, 1);
  border-radius: 8rpx;
  box-sizing: border-box;
  padding: 26rpx 32rpx;
  font-size: 30rpx;
}
.start_group{
  height: 108rpx;
  width: 100%;
  display: flex;
  align-items: center;
}
.mintitle{
  font-size: 28rpx;
  color: rgba(44, 44, 44, 1);
  margin-right: 16rpx;
}
.icon-xingxing{
  font-size: 20rpx;
  color: rgba(255, 153, 73, 1);
  margin-right: 10rpx;
}
.button {
  width: 656rpx;
  height: 88rpx;
  background: rgba(96, 234, 243, 1);
  border-radius: 8rpx;
  margin:0 auto;
  margin-top: 80rpx;
  font-size: 36rpx;
  text-align: center;
  line-height: 88rpx;
  color: rgba(255, 255, 255, 1);
}