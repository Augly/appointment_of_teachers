page {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 0rpx 32rpx;
}

.step_group {
  /* width: 500rpx; */
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  margin: 0 auto;
  margin-top: 48rpx;
}

.step {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  border: 2rpx solid rgba(153, 153, 153, 1);
  font-size: 32rpx;
  color: rgba(153, 153, 153, 1);
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
}

.step_active {
  color: rgba(255, 255, 255, 1);
  background: rgba(96, 234, 243, 1);
  border: 2rpx solid rgba(96, 234, 243, 1);
}

.line {
  width: 152rpx;
  height: 0rpx;
  border: 2rpx dashed rgba(153, 153, 153, 1);
}

.tip {
  color: rgba(51, 51, 51, 1);
  font-size: 26rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  margin-top: 80rpx;
}

.tip_click {
  color: rgba(96, 234, 243, 1);
  font-size: 26rpx;
  text-decoration: underline;
}

.list {
  width: 100%;
  height: 80rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  margin-top: 20rpx;
}

.list_title {
  font-size: 32rpx;
  color: rgba(44, 44, 44, 1);
}

.inp {
  width: 490rpx;
  height: 100%;
  font-size: 32rpx;
  /* color: rgba(153, 153, 153, 1); */
}

.button {
  width: 688rpx;
  height: 88rpx;
  background: rgba(96, 234, 243, 1);
  border-radius: 8rpx;
  text-align: center;
  line-height: 88rpx;
  color: rgba(255, 255, 255, 1);
  font-size: 34rpx;
  margin-top: 112rpx;
}

.up_wrap {
  width: 208rpx;
  height: 208rpx;
  border: 2rpx dashed rgba(204, 204, 204, 1);
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -ms-flex-direction: column;
  flex-direction: column;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  margin-top: 65rpx;
}

.icon-hao {
  font-size: 60rpx;
  color: rgba(183, 183, 183, 0.7);
}

.title {
  font-size: 24rpx;
  color: rgba(183, 183, 183, 0.7);
  margin-top: 30rpx;
}